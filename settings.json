{
    "better-comments.multilineComments": true,
    "breadcrumbs.enabled": true,
    "diffEditor.codeLens": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.fontSize": 15,
    "editor.fontFamily": "MesloLGS NF",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "editor.lineHeight": 22,
    "editor.parameterHints.enabled": false,
    "editor.renderLineHighlight": "gutter",
    "editor.rulers": [
        100,
        140
    ],
    "editor.suggestSelection": "first",
    "editor.tabSize": 4,
    "editor.wordWrap": "wordWrapColumn",
    "explorer.compactFolders": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "onFocusChange",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": true,
        "markdown": false
    },
    "go.autocompleteUnimportedPackages": true,
    "go.formatTool": "goimports",
    "go.lintOnSave": "package",
    "go.testOnSave": true,
    "go.testFlags": [
        "-v"
    ],
    "go.toolsManagement.autoUpdate": true,
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.suggest.snippetsPreventQuickSuggestions": true
    },
    "json.maxItemsComputed": 50000,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pycodestyleEnabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.fontSize": 15,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path.mac": "/Users/luiza.des.ferreira/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vscode-kubernetes.kubectl-path.mac": "/Users/luiza.des.ferreira/.vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.minikube-path.mac": "/Users/luiza.des.ferreira/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.labelFormat": "short",
    "workbench.editor.enablePreview": true,
    "workbench.activityBar.visible": true,
    "workbench.editor.untitled.hint": "hidden"
}